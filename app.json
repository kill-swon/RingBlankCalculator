{
  "expo": {
    "name": "Ring Blank Calculator",
    "slug": "RingBlankCalculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/RingBlankCalculatorIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/RingBlankCalculatorIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.killswon.RingBlankCalculator"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/RingBlankCalculatorFavicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/RingBlankCalculatorIcon.png",
          "imageWidth": 100,
          "resizeMode": "contain",
          "backgroundColor": "#FEF7FF",
          "dark": {
            "image": "./assets/images/RingBlankCalculatorIcon.png",
            "imageWidth": 100,
            "resizeMode": "contain",
            "backgroundColor": "#141218"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8013a48-9833-4bcb-b5b3-ade47b36ea45"
      }
    }
  }
}